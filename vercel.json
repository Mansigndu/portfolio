{
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": ".",
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/image/(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/image/:path*",
      "destination": "/public/image/:path*"
    }
  ],
  "rewrites": [
    {
      "source": "/(?!image|public|.*\\.)",
      "destination": "/index.html"
    }
  ]
}
